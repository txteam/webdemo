<?xml version="1.0" encoding="UTF-8"?>
<configs>
	<catalog code="paoding" name="庖丁分词配置">
		<catalog code="paoding.analyzer">
			<config>
				<name>mode</name>
				<code>paoding.analyzer.mode</code>
				<value>most-words</value>
			</config>
			<config>
				<name>dictionaries.compiler</name>
				<code>paoding.analyzer.dictionaries.compiler</code>
				<value>net.paoding.analysis.analyzer.impl.MostWordsModeDictionariesCompiler</value>
			</config>
		</catalog>
		<catalog code="paoding.dic">
			<config>
				<name>home</name>
				<code>paoding.dic.home</code>
				<value>classpath:context/paoding/dic</value>
			</config>
			<config>
				<name>detector.interval</name>
				<code>paoding.dic.detector.interval</code>
				<value>60</value>
			</config>
		</catalog>
		<catalog code="paoding.knife">
			<config>
				<name>letterKnife</name>
				<code>paoding.knife.class.letterKnife</code>
				<value>net.paoding.analysis.knife.LetterKnife</value>
			</config>
			<config>
				<name>numberKnife</name>
				<code>paoding.knife.class.numberKnife</code>
				<value>net.paoding.analysis.knife.NumberKnife</value>
			</config>
			<config>
				<name>cjkKnife</name>
				<code>paoding.knife.class.cjkKnife</code>
				<value>net.paoding.analysis.knife.CJKKnife</value>
			</config>
		</catalog>
	</catalog>
</configs>